<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.miwgroningen.se.ch9.vincent.controller.ShowLogController">

    <padding>
        <Insets bottom="50.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label text="TimeLogger" />
    <Label fx:id="startTime" />
    <Label fx:id="endTime" />
    <Button onAction="#toLogScene" text="Go back to Logging">
      <VBox.margin>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
      </VBox.margin></Button>
    <Button onAction="#toLogHistoryScene" text="Show log history">
      <VBox.margin>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
      </VBox.margin></Button>
</VBox>
