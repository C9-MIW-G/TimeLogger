<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="nl.miwgroningen.se.ch9.vincent.controller.ManageProjectsController"
            prefHeight="400.0" prefWidth="600.0">

    <VBox alignment="TOP_CENTER" layoutX="250.0" layoutY="100.0" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <ListView fx:id="projectListView" prefHeight="450.0" prefWidth="600.0"/>
        <HBox alignment="CENTER" prefHeight="50.0">
            <Button mnemonicParsing="false" onAction="#deleteProject" text="Delete"/>
            <Button mnemonicParsing="false" onAction="#editProject" text="Edit"/>
            <Button mnemonicParsing="false" onAction="#createProject" text="New"/>
        </HBox>
    </VBox>
</AnchorPane>
